    },
    "patentis": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patenti.patenti",
      "mappedBy": "candidato"
    },
    "linguas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::linguas.linguas",
      "mappedBy": "candidato"
    },
    "tests": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::test.test",
      "mappedBy": "candidatoes"
